{
    "name": "nutech-integrasi",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "start": "npm run migrate && npm run seed && node ./bin/www",
        "dev": "nodemon ./bin/www",
        "migrate": "node src/database/migration-system/migrator.js migrate",
        "migrate:rollback": "node src/database/migration-system/migrator.js rollback",
        "migrate:fresh": "node src/database/migration-system/migrator.js fresh",
        "seed": "node src/database/migration-system/seeder.js run",
        "seed:specific": "node src/database/migration-system/seeder.js run-specific"
    },
    "dependencies": {
        "bcrypt": "^5.1.1",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "debug": "^4.3.7",
        "dotenv": "^16.4.5",
        "express": "^4.21.2",
        "joi": "^17.13.3",
        "jsonwebtoken": "^9.0.2",
        "morgan": "^1.10.1",
        "multer": "^2.0.1",
        "mysql2": "^3.11.4",
        "swagger-ui-express": "^5.0.1",
        "uuid": "^10.0.0"
    },
    "devDependencies": {
        "nodemon": "^3.1.10"
    }
}
